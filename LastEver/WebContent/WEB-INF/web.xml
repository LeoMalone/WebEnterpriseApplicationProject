<?xml version="1.0" encoding="UTF-8"?>
<web-app
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
	version="2.5">
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>servlets.LogoutServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>servlets.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CreateAccountServlet</servlet-name>
		<servlet-class>servlets.CreateAccountServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>CreateAccountServlet</servlet-name>
		<url-pattern>/createAccount</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AdminUsers</servlet-name>
		<servlet-class>servlets.AdminUsersServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AdminUsers</servlet-name>
		<url-pattern>/adminUsers</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>StandingsServlet</servlet-name>
		<servlet-class>servlets.StandingsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StandingsServlet</servlet-name>
		<url-pattern>/standings</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ScheduleServlet</servlet-name>
		<servlet-class>servlets.ScheduleServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ScheduleServlet</servlet-name>
		<url-pattern>/schedule</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ResultsServlet</servlet-name>
		<servlet-class>servlets.ResultsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ResultsServlet</servlet-name>
		<url-pattern>/results</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>StatisticsServlet</servlet-name>
		<servlet-class>servlets.StatisticsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StatisticsServlet</servlet-name>
		<url-pattern>/statistics</url-pattern>
	</servlet-mapping>

	<display-name>LastEver</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>